{
  "mod": "company.mod.json",
  "select": ["id", "com_name", "com_desc", "flag"],
  "where": [
    {
      "name": "com_name",
      "op": "like",
      "on": "len(com_name)!=0"
    },
    {
      "name": "flag",
      "op": "eq",
      "on": "flag!=-1",
      "default": 0
    }
  ],
  "order": "id desc"
}